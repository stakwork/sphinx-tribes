{
  "name": "catcher",
  "version": "0.1.0",
  "prettier": "prettier.json",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:tribes:docker": "PORT=23000 react-scripts start",
    "start:tribes:cluster": "PORT=13000 react-scripts start",
    "start:people": "PORT=3007 react-scripts start",
    "start:people:docker": "PORT=23007 react-scripts start",
    "start:people:cluster": "PORT=13007 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --max-warnings 410 --ext .ts --ext .tsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier --config .prettierrc.json -w src/*.ts src/*.tsx src/**/*.ts src/**/*.tsx src/**/**/*.ts src/**/**/*.tsx"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "dependencies": {
    "@elastic/datemath": "^5.0.2",
    "@elastic/eui": "^64.0.4",
    "@loomhq/loom-embed": "^1.4.0",
    "@loomhq/record-sdk": "^2.35.16",
    "@material/react-material-icon": "^0.15.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.0",
    "bootstrap": "^4.5.0",
    "dateformat": "^3.0.3",
    "formik": "^2.2.6",
    "fuse.js": "^5.2.3",
    "lodash": "^4.17.15",
    "mobx": "^5.15.4",
    "mobx-persist": "^0.4.1",
    "mobx-react-lite": "^1.5.2",
    "moment": "^2.24.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^11.3.2",
    "react-markdown": "^8.0.3",
    "react-qr-svg": "^2.2.2",
    "react-qrcode-logo": "^2.5.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "react-select": "^5.1.0",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "remark-rehype": "^10.1.0",
    "styled-components": "^5.1.0",
    "yup": "^0.32.9"
  },
  "eslintConfig": {
    "extends": ["react-app", "eslint:recommended", "plugin:react/recommended"],
    "rules": {
      "react-hooks/rules-of-hooks": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "react/prop-types": "off",
			"no-unused-vars": "off",
			"no-extra-boolean-cast": "warn",
			"no-useless-escape": "warn",
			"no-prototype-builtins": "warn",
			"no-empty-pattern": "warn",
			"no-empty": "warn",
			"no-unreachable": "warn",
			"no-useless-catch": "warn",
			"react/jsx-closing-bracket-location": ["warn", "tag-aligned"],
			"react/jsx-closing-tag-location": "warn",
			"jsx-quotes": ["error", "prefer-double"],
			"prefer-const": "warn",
			"no-const-assign": "warn",
			"no-var": "warn",
			"prefer-destructuring": "warn",
			"prefer-template": "warn",
			"func-style": ["warn", "expression"],
			"react/self-closing-comp": "warn",
			"react/jsx-no-target-blank": "warn",
			"react/jsx-key": "warn",
			"react/no-children-prop": "warn",
			"react/no-unescaped-entities": "warn"
		}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.27",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "bootstrap": "^4.5.0",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "react-error-overlay": "6.0.9",
    "typescript": "4.5.3"
  }
}
